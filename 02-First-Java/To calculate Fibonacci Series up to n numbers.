import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        int n = in.nextInt();

        if (n == 0) {
            System.out.print(0);
        } else if (n == 1) {
            System.out.print(1);
        } else {
            int a = 0, b = 1, sum = 0;
            for (int i = 2; i <= n; i++) {
                sum = a + b;
                a = b;
                b = sum;
            }
            System.out.print(sum);
        }

        in.close();
    }
}
