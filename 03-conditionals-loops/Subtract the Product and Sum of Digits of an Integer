import java.util.Scanner;

class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number: ");
        int num = sc.nextInt();
        
        int product = 1, sum = 0;

        while(num > 0){
            int rem = num % 10;
            product *= rem;
            sum += rem;
            num /= 10;
        }

        System.out.println("Answer = " + (product-sum));
        sc.close();
    }
}
